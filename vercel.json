{
    "version": 2,
    "builds": [
        {
            "src": "dist",
            "use": "@vercel/static"
        },
        {
            "src": "server/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "dist/$1"
        }
    ]
}
